############################################################
# Rotates a block
############################################################

execute if block ~ ~ ~ beehive[honey_level=0] run function tcc:item/wrench/rotate/block/beehive/honey_level_0
execute if block ~ ~ ~ beehive[honey_level=1] run function tcc:item/wrench/rotate/block/beehive/honey_level_1
execute if block ~ ~ ~ beehive[honey_level=2] run function tcc:item/wrench/rotate/block/beehive/honey_level_2
execute if block ~ ~ ~ beehive[honey_level=3] run function tcc:item/wrench/rotate/block/beehive/honey_level_3
execute if block ~ ~ ~ beehive[honey_level=4] run function tcc:item/wrench/rotate/block/beehive/honey_level_4
execute if block ~ ~ ~ beehive[honey_level=5] run function tcc:item/wrench/rotate/block/beehive/honey_level_5
